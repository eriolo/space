{
  "extends": ["react-app", "prettier"],
  "env": {
    "jest": true
  },
  "plugins": ["jest", "no-only-tests", "import", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/order": ["warn", { "groups": ["builtin", "external", "internal", "parent", "sibling", "index"] }],
    "no-only-tests/no-only-tests": "error"
  }
}
